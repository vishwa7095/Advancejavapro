package com.List;
import java.util.*;
public class Example1 {

	public static void main(String[] args) {
		ArrayList<Integer> al=new ArrayList<Integer>();
		al.add(10);
		al.add(20);
		al.add(30);
		al.add(40);
		System.out.println("before remove all elements"+al);
		al.clear();
		System.out.println("after remove all elements");

	}

}
