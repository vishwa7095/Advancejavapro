package com.Assignment1;
import java.util.*;
public class Example1 {

	public static void main(String[] args) {
		List<Players> pl =new ArrayList<Players>();
		pl.add(new Players(101,"DHONI",31,"CSK"));
		pl.add(new Players(102,"VIRAT KOHILI",28,"CSK"));
		pl.add(new Players(103,"RAHUL",22,"CSK"));
		pl.add(new Players(104,"RASHID KHAN",25,"CSK"));
		pl.add(new Players(105,"RAINA",21,"CSK"));
		pl.add(new Players(106,"ANDRE RUSSEL",35,"CSK"));
			
		String sn="dhoni";
		boolean found=false;
		for(Players x:pl)
		{
			if(x.getPlayerName.equals(sn))
			{
				found = true;
                System.out.println("Player found:");
                System.out.println(pl);
                break; // Exit the loop once player is found
            }
        }

        // If player not found, print a message
        if (!found) {
            System.out.println("Player with name '" + sn + "' not found.");
        }
			}
		
	}


