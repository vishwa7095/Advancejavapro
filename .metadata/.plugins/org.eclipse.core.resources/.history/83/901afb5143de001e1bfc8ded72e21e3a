package com.Assignment1;

public class Players {
	private int PlayerID;
	private String PlyerName;
	private int Age;
	private String Team;
	Players(int playerID, String plyerName, int age, String team) {
		super();
		PlayerID = playerID;
		PlyerName = plyerName;
		Age = age;
		Team = team;
	}
	public int getPlayerID() {
		return PlayerID;
	}
	public void setPlayerID(int playerID) {
		PlayerID = playerID;
	}
	public String getPlyerName() {
		return PlyerName;
	}
	public void setPlyerName(String plyerName) {
		PlyerName = plyerName;
	}
	public int getAge() {
		return Age;
	}
	public void setAge(int age) {
		Age = age;
	}
	public String getTeam() {
		return Team;
	}
	public void setTeam(String team) {
		Team = team;
	}
	

}
