package com.arryas;

public class Example1 {

	public static void main(String[] args) {
		int i,maxelement;
		int arr[]= {32,56,78,65,98,65,45};
		maxelement=arr[0];
		for(i=0;i<arr.length;i++)
		{
			 
			if(arr[i]>maxelement)
			{
			arr[i]=maxelement;
	}
		}
		System.out.println(maxelement);


}
}