package com.arryas;

public class Example1 {

	public static void main(String[] args) {
		int i,smallelement,maxelement;
		int arr[]= {32,56,78,65,98,65,45};
		smallelement=arr[0];
		maxelement=arr[0];
		for(i=1;i<arr.length;i++)
		{
			 
			if(arr[i]<smallelement)
			{
			smallelement=arr[i];
	}
			if(arr[i]>maxelement)
			{
				maxelement=arr[i];
			}
		}
		System.out.println(smallelement);


}
}