package com.List;
import java.util.*;
public class Employeedetails {

	public static void main(String[] args) {
		List<Employee> EMP=new ArrayList<Employee>();
		EMP.add(new Employee(101,"bharath",50000));
		EMP.add(new Employee(102,"aslam",50000));
		EMP.add(new Employee(102,"manikanta",75000));
		EMP.add(new Employee(101,"saibabu",75000));
		for(Employee x:EMP)
		{
			System.out.println(x);
		}

	}

}
